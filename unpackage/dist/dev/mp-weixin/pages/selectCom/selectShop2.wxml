<view class="shop_select_body"><view class="city_search_sy"><view class="full_search_box"><view class="full_search"><view class="iconfont iconsousuo1"></view><input class="input" type="text" placeholder="请输入店铺名称" data-event-opts="{{[['input',[['__set_model',['','seaText','$event',[]]]]]]}}" value="{{seaText}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" hidden="{{!(seaText!='')}}" class="iconfont iconguanbi" bindtap="__e"></view></view><view class="text">搜索</view></view></view><view class="h20ccc"></view><block wx:for="{{commodityList}}" wx:for-item="tDatial" wx:for-index="index" wx:key="id"><view class="product_item_sy"><view class="left"><image class="img" src="{{'../../static/img/'+tDatial.url}}" mode="widthFix"></image></view><view class="right"><view class="p1"><view class="title">{{''+tDatial.title+''}}</view><view hidden="{{!(tDatial.tj)}}" class="tj">推荐</view></view><view class="p2"><grade-com-sy vue-id="{{'ad5e77ce-1-'+index}}" num="{{tDatial.star}}" bind:__l="__l"></grade-com-sy><view class="distance">{{''+tDatial.distance+'km'}}</view></view><view class="p3"><view class="card" style="{{'background-color:'+(tDatial.color)+';'}}">{{''+tDatial.type+''}}</view><view class="{{['mount_pay',(tDatial.num<40)?'hidden':'']}}">{{'月消'+tDatial.num+''}}</view></view><view class="{{['p4',(tDatial.info=='')?'hidden':'']}}">{{''+tDatial.info+''}}</view><view class="p5"><view class="iconfont iconlocation"></view><view class="address">{{''+tDatial.address+''}}</view></view></view></view></block></view>